<?xml version="1.0"?>
<extension key="org.asmac.paymentdescriptionmembership" type="module">
  <file>paymentdescriptionmembership</file>
  <name>ASMAC Payment Description Show Renewal and Membership Type</name>
  <description>Rework description sent to PayPal and Stripe to include signup vs. renewal and Membership Type using alterPaymentProcessorParams hook.</description>
  <license>BSD-2-Clause</license>
  <maintainer>
    <author>Jeff Kellem</author>
    <email>tech@asmac.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/asmac-org/org.asmac.paymentdescriptionmembership</url>
    <url desc="Documentation">https://github.com/asmac-org/org.asmac.paymentdescriptionmembership</url>
    <url desc="Support">http://asmac.org/contact/</url>
    <url desc="Licensing">http://opensource.org/licenses/BSD-2-Clause</url>
  </urls>
  <releaseDate>2023-05-22</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.61</ver>
  </compatibility>
  <comments>debug log on</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Paymentdescriptionmembership</namespace>
    <format>23.02.1</format>
    <angularModule>crmPaymentdescriptionmembership</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
